spring:
  datasource:
    url: jdbc:h2:mem:testdb
    username: sa
    password:
    driverClassName: org.h2.Driver
  jpa:
    properties.hibernate.dialect: org.hibernate.dialect.H2Dialect
    hibernate.ddl-auto: update
  security:
    oauth2:
      client:
        registration:
          google:
            client-id: <google console client id>
            client-secret: <google console client secret>
            scope:
              - email
              - profile